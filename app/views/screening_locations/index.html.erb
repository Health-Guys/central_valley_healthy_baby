<h1>Listing screening_locations</h1>

<table class="table table-striped table-with-link-column">
  <thead>
    <tr>
      <th>Disease</th>
      <th>Street</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Zipcode</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @screening_locations.each do |screening_location| %>
      <tr>
        <td><%= screening_location.disease %></td>
        <td><%= screening_location.street %></td>
        <td><%= screening_location.name %></td>
        <td><%= screening_location.phone %></td>
        <td><%= screening_location.zipcode %></td>
        <td><%= screening_location.longitude %></td>
        <td><%= screening_location.latitude %></td>
        <td><%= screening_location.state %></td>
        <td><%= link_to 'Show', screening_location %></td>
        <td><%= link_to 'Edit', edit_screening_location_path(screening_location) %></td>
        <td><%= link_to 'Destroy', screening_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Screening location', new_screening_location_path %>
